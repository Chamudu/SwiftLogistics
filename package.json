{
  "name": "swiftlogistics",
  "version": "1.0.0",
  "description": "A middleware architecture project integrating heterogeneous systems (CMS, ROS, WMS) for logistics management.",
  "main": "index.js",
  "type": "module",
  "private": true,
  "workspaces": [
    "mock-services/*",
    "adapters/*",
    "shared/*",
    "workers/*",
    "auth-service",
    "order-service",
    "websocket-service",
    "api-gateway"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:mock-ros": "npm run dev --workspace=mock-ros",
    "dev:mock-cms": "npm run dev --workspace=mock-cms",
    "dev:mock-wms": "npm run dev --workspace=mock-wms",
    "dev:all-mocks": "npm run dev:mock-ros & npm run dev:mock-cms & npm run dev:mock-wms"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chamudu/SwiftLogistics.git"
  },
  "keywords": [
    "middleware",
    "microservices",
    "rabbitmq",
    "soap",
    "rest",
    "tcp",
    "logistics"
  ],
  "author": "Chamudu Hansana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chamudu/SwiftLogistics/issues"
  },
  "homepage": "https://github.com/Chamudu/SwiftLogistics#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.24",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  },
  "dependencies": {
    "express-rate-limit": "^8.2.1",
    "socket.io": "^4.8.3"
  }
}